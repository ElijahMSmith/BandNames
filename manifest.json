{
    "name": "Band Name Suggestions",
    "version": "1.0",
    "description": "Suggests possible band names as your Discord username!",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "clipboardRead", "clipboardWrite", "storage", "webNavigation", "tabs"],
    "background": {
        "scripts": ["installScripts.js",
                    "urlListen.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "suggestion_popup.html",
        "default_icon": {
          "16": "images/BandNames_16.png",
          "32": "images/BandNames_32.png",
          "48": "images/BandNames_48.png",
          "128": "images/BandNames_128.png"
        }
    },
    "icons": {
      "16": "images/BandNames_16.png",
      "32": "images/BandNames_32.png",
      "48": "images/BandNames_48.png",
      "128": "images/BandNames_128.png"
    },
    "options_page": "options.html"
}